{
  "filepath": {
    "sourceMDDir": "./configurables/source_md",
    "destHtmlDir": "./hanamark_docs",
    "templatePath": "./configurables/templates",
    "sourceStaticFiles": "./configurables/static",
    "mdAssetsSourcePath": "./configurables/source_md/assets/",
    "mdAssetsDestPath": "./hanamark_docs/assets/"
  },
  "indexHomepageHtml": {
    "type": "page",
    "name": "index.html",
    "titletag":"this text will be added inside title tag of index page"

  },
  "logger": {
    "filepath": "hanamark.logs",
    "level": "debug"
  },
  "rss": {
    "isRssEnabled": false
  },
  "tags": false,
  "sortFilesByCreatedOn": true,
  "servePort": "3000",
   "opengraph": {
    "enabled": false,
    "siteName": "My Site",
    "baseUrl": "https://example.com",
    "defaultImage": "/static/images/og-default.png",
    "imageWidth": "1200",
    "imageHeight": "630",
    "twitterCard": "summary_large_image"
  }
}
